<Window x:Class="Peeky_Blinkers.ErrorDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Peeky_Blinkers"
        mc:Ignorable="d"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen"
        Title="ErrorDialog" Height="250" Width="400">
    <Grid>
        <Grid>
            <Image Source="/error.jpg" Stretch="Fill"/>
            <TextBlock Text="Peeky Blinkers already running !" Margin="96,217,114,-12"/>
            <Button Name="BtnClose" Content="Close" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="270,212,0,0" Height="25" Width="60"/>
        </Grid>
    </Grid>
</Window>
